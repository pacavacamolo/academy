class HelloWorld{
    public static void main(String []args){
    System.out.println("NOD: " + gcd(7,19));
       
     System.out.println("NOK: " + lcm(6,15));
    }
         ///NOD
   // NOD
   
     static long gcd(long a,long b){
	
      return b == 0 ? a : gcd(b,a % b);	
	
    }
   //NOK
    
    static long lcm(long a,long b){
	 
      return a / gcd(a,b) * b;
   
 }

 }   
